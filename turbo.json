{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "db:start": {
      "cache": false
    },
    "db:stop": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "db:cleanup": {
      "cache": false
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "^db:generate"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "dependsOn": [
        "build",
        "^db:generate"
      ],
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "cache": false,
      "persistent": true
    },
    "test": {},
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "e2e:run": {
      "cache": false
    },
    "e2e:report": {}
  }
}